<Window x:Class="WpfApp25.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp25" xmlns:dg="clr-namespace:PropertyTools.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <dg:DataGrid AutoGenerateColumns="False" ItemsSource="{Binding Epmls}">
            <dg:DataGrid.ColumnDefinitions>
                <dg:ColumnDefinition PropertyName="Name" Header="Name"/>
            </dg:DataGrid.ColumnDefinitions>
            
            
        </dg:DataGrid>
    </Grid>
</Window>
